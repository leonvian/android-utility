package br.com.lvc.utility.screen;

import android.view.View;

public class ScreenView {
	
	private View view;
	private boolean obrigatory;
	
	public ScreenView() {
	
	}
	
	

	public ScreenView(View view, boolean obrigatory) {
		super();
		this.view = view;
		this.obrigatory = obrigatory;
	}



	public View getView() {
		return view;
	}

	public void setView(View view) {
		this.view = view;
	}

	public boolean isObrigatory() {
		return obrigatory;
	}

	public void setObrigatory(boolean obrigatory) {
		this.obrigatory = obrigatory;
	}
	
	

}
